page {
  background-color: #f5f5f5;
}

.container .img-wrapper swiper {
  height: 680rpx;
}

.container .img-wrapper swiper image {
  width: 100%;
}

.container .synopsis-wrapper {
  position: relative;
  margin-bottom: 20rpx;
  background-color: #fff;
}

.container .synopsis-wrapper .info-wrapper {
  padding: 24rpx 0 28rpx 30rpx;
  width: 540rpx;
}

.container .synopsis-wrapper .info-wrapper .title {
  color: #333333;
  font-size: 32rpx;
  margin-bottom: 18rpx;
}

.container .synopsis-wrapper .info-wrapper .subtitle {
  color: #999999;
  font-size: 28rpx;
  margin-bottom: 34rpx;
}

.container .synopsis-wrapper .info-wrapper .discount {
  color: #157658;
  font-size: 44rpx;
  margin-right: 30rpx;
}

.container .synopsis-wrapper .info-wrapper .price {
  font-size: 32rpx;
  color: #999999;
  text-decoration: line-through;
}

.container .synopsis-wrapper .info-wrapper > text {
  display: block;
}

.container .synopsis-wrapper .operator-wrapper {
  position: absolute;
  top: 38rpx;
  right: 30rpx;
  border-left: 2rpx dotted #DCEBE6;
}

.container .synopsis-wrapper .operator-wrapper .share {
  margin-left: 28rpx;
}

.container .synopsis-wrapper .operator-wrapper .share button {
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  text-align: center;
  width: 50rpx !important;
  height: 120rpx !important;
  background: #DCEBE6;
}

.container .synopsis-wrapper .operator-wrapper .share button text {
  color: #157658;
  font-size: 20rpx;
}

.container .coupon-wrapper {
  padding: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.container .coupon-wrapper .coupon-list {
  display: flex;
}

.container .coupon-wrapper .coupon-list .item {
  font-size: 20rpx;
  margin-right: 20rpx;
  color: #157658;
  line-height: 48rpx;
  text-align: center;
  width: 200rpx;
  background-color: #DCEBE6;
}

.container .coupon-wrapper .more {
  font-size: 28rpx;
  color: #333333;
}

.container .coupon-wrapper .more text:first-child {
  margin-right: 20rpx;
}

.container .explain-wrapper {
  padding: 10rpx 10rpx 0 10rpx;
  background-color: #fff;
}

.container .explain-wrapper .explain-list {
  border: 2rpx dotted #DCEBE6;
  margin-bottom: 10rpx;
}

.container .explain-wrapper .explain-list .explain-item {
  margin: 0 30rpx;
  line-height: 78rpx;
  color: #333333;
  font-size: 28rpx;
  border-bottom: 2rpx solid #DCEBE6;
}

.container .explain-wrapper .explain-list .explain-item:last-child {
  border-bottom: none;
}

.picture-grid {
  display: flex;
  justify-content: space-between;
}

.picture-grid .flex {
  display: flex;
}

.picture-grid .flex:nth-child(2) {
  flex-direction: column;
  justify-content: space-between;
}

.picture-grid .left image {
  width: 344rpx;
}

.picture-grid .right image {
  width: 400rpx;
}
